while ((line = get_next_line(fd)))
{
    int i = 0;
    while (line[i] == ' ' || line[i] == '\t')
        i++;

    if (line[i] == 'F' || line[i] == 'C')
    {
        if (parse_color(line + i + 1, 1))
            error = 1;
    }
    else if (line[i] == 'N' || line[i] == 'S' ||
             line[i] == 'W' || line[i] == 'E')
    {
        check_texture_line(&tex, line + i);
    }
    else if (line[i] != '\0') // first real map line
    {
        map = get_map(line, fd);
        break; // after this, get_map reads the rest of the map
    }

    free(line);
}
